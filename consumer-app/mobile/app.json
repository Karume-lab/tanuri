{
	"expo": {
		"name": "bna",
		"slug": "bna",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/core/taska-ios-dark.png",
		"scheme": "tanuri",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/core/taska-splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#0A0A0A"
		},
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.doe.tanuri"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/core/taska-adaptive-dark.png",
				"backgroundColor": "#0A0A0A"
			},
			"edgeToEdgeEnabled": true,
			"package": "com.doe.tanuri"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/core/taska-logo.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/core/taska-splash.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#0A0A0A"
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		}
	}
}
