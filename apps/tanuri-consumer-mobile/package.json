{
  "name": "tanuri-consumer-mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "get-ip": "bun run src/scripts/get-ip.ts",
    "dev": "bun run get-ip && expo start --dev-client",
    "start": "bun run get-ip && expo start --go",
    "android": "bun run get-ip && expo prebuild --clean --no-install && expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build": "bun run get-ip && expo prebuild --clean --no-install && ./android/gradlew -p android assembleRelease",
    "install-apk": "adb install ./android/app/build/outputs/apk/release/app-release.apk",
    "build:install-apk": "bun run build && bun run install-apk",
    "doctor": "expo-doctor --verbose",
    "update-packages": "expo install --check"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@hookform/resolvers": "^5.2.1",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@shopify/flash-list": "2.0.2",
    "expo": "~54.0.11",
    "expo-blur": "~15.0.7",
    "expo-constants": "~18.0.9",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.9",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.8",
    "lucide-react-native": "^0.525.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "expo-doctor": "^1.17.7"
  },
  "peerDependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0"
  },
  "private": true
}
